      // Content Wrapper. Contains page content
      .content-wrapper
        // Content Header (Page header)
        br
        // Main content
        section.content
          .box-header
            // main cintainer start
            .row
              // main cintainer start
              .panel.panel-primary
                .panel-heading
                  h3.panel-title
                    | Test Users
                .panel-body
                  .panel.panel-info
                    .panel-heading
                      h3.panel-title
                        | Add New User
                        .btn-group.pull-right
                    .panel-body
                      #accordion.panel-group(role='tablist', aria-multiselectable='true')
                        // Accordion one start
                        .panel.panel-default
                          .panel-body
                            form.form-horizontal
                              fieldset
                                // Text input
                                .form-group
                                  label.col-md-4.control-label(for='new_user_name') Name
                                  .col-md-4
                                    input#new_user_name.form-control.input-md(name='new_user_name', placeholder='Enter New User Name', required='', type='text')
                                // Select Basic
                                .form-group
                                  label.col-md-4.control-label(for='selectbasic') Operating System
                                  .col-md-4
                                    select#selectbasic.form-control(name='selectbasic')
                                      option(value='Select') Select
                                      option(value='iOS') iOS
                                      option(value='Android') Android
                                // Text input
                                .form-group
                                  label.col-md-4.control-label(for='device_id') Device ID
                                  .col-md-4
                                    input#device_id.form-control.input-md(name='device_id', placeholder='Device ID', required='', type='text')
                                // Button
                                .form-group
                                  label.col-md-4.control-label(for='singlebutton')
                                  .col-md-4
                                    button#singlebutton.btn.btn-success(name='singlebutton') Add New User
